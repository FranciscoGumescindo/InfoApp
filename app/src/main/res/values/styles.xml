<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    </style>

    <style name="AppTheme2" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/green</item>
        <item name="colorPrimaryDark">@color/barraIsc</item>
    </style>

    <style name="AppThemeIi" parent="Theme.AppCompat.DayNight.DarkActionBar">
        <item name="colorPrimary">@color/amarilloF</item>
        <item name="colorPrimaryDark">@color/amarillo</item>
    </style>

    <style name="AppThemeIge" parent="Theme.AppCompat.DayNight.DarkActionBar">
        <item name="colorPrimary">@color/purplef</item>
        <item name="colorPrimaryDark">@color/purple</item>
    </style>

</resources>
